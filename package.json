{
  "name": "early-return",
  "displayName": "Early Return",
  "publisher": "AbdoArwish",
  "repository":"https://github.com/AbdoHema2016/earlyReturn.git",
  "description": "changes if else statements to early return pattern",
  "version": "0.0.1",
  "author": {"name": "Abdo Arwish"},
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Formatters",
    "Programming Languages"
  ],
  "icon": "ER.png",
  "activationEvents": [
    "onCommand:early-return.earlyReturn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "early-return.earlyReturn",
        "title": "Early return",
        "category": "early-return"
      }
    ],
    "keybindings":[
      {
        "command": "early-return.earlyReturn",
        "key": "ctrl+shift+e",
        "mac": "cmd+shift+e"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
